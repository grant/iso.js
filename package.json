{
  "name": "iso",
  "version": "0.0.1",
  "description": "A library for rendering isomorphic graphics",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "babel --watch=./src --out-dir=./build/",
    "watch-browser": "watchify ./build/index.js -d -o ./build/browser-iso.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grant/iso.git"
  },
  "browserify-shim": {
    "three": "THREE"
  },
  "browser": {
    "three": "./node_modules/three/three.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "author": "Grant Timmerman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grant/iso/issues"
  },
  "homepage": "https://github.com/grant/iso#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.4",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify-shim": "^3.2.2",
    "flow-bin": "^0.22.1",
    "immutable": "^3.7.6",
    "three": "^0.75.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {}
}
